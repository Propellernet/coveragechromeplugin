{
  "manifest_version": 2,

  "name": "CoverageBook",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",
  "browser_action": {
    "default_icon": "cbook-logo.png",
    "default_popup": "popup.html",
    "default_title": "Coverage Capture"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}